{
  "plugins": ["node_modules/jsdoc-vuejs", "node_modules/jsdoc-vuex-plugin"],
  "source": {
    "include": "src",
    "includePattern": "\\.(vue|js)$"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "node_modules/minami"
  }
}
