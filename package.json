{
  "name": "av2-linked-up",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build:report": "vue-cli-service build --report",
    "cover": "cross-env NODE_ENV=e2e nyc --silent npm run test:e2e",
    "cover:merge": "npx cypress-jest-coverage-merge",
    "dev": "npm run docs && npm run lint && npm -s run test:unit",
    "docker": "docker-compose --env-file .env.local up --build",
    "docs": "jsdoc -c jsdoc.json",
    "lh": "lighthouse http://localhost:3000 --screenEmulation.disabled --throttling-method=provided --no-emulated-user-agent --output html --output-path ./lighthouse/report.html --save-assets",
    "preview": "npm run build && serve -s dist"
  },
  "dependencies": {
    "@sentry/tracing": "^6.14.0",
    "@sentry/vue": "^6.14.0",
    "bootstrap-icons": "^1.6.1",
    "core-js": "^3.6.5",
    "firebase": "^8.9.1",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.11",
    "@popperjs/core": "^2.10.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-require-context": "^0.1.1",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "cypress-browser-permissions": "^1.0.5",
    "cypress-file-upload": "^5.0.8",
    "cypress-jest-coverage-merge": "^1.0.2",
    "date-fns": "^2.25.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "jsdoc": "^3.6.7",
    "jsdoc-vuejs": "^4.0.0",
    "jsdoc-vuex-plugin": "^1.0.0",
    "lodash": "^4.17.21",
    "minami": "^1.2.3",
    "nyc": "^15.1.0",
    "sass": "^1.43.3",
    "sass-loader": "^10.2.0",
    "style-resources-loader": "^1.4.1",
    "typescript": "~3.9.3",
    "vue-cli-plugin-style-resources-loader": "~0.1.5",
    "vue-jest": "^5.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "transform": {
      "^.+\\.vue$": "vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}"
    ],
    "coverageDirectory": "./tests/coverage/jest",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/test/",
      "/utils/",
      "main.js",
      "/store/",
      "/router/"
    ]
  },
  "nyc": {
    "report-dir": "./tests/coverage/cypress",
    "extension": [
      ".js",
      ".vue"
    ]
  }
}
